  "devDependencies": {
    "shx": "^0.3.2",
    "@ui5/cli": "^2.4.1",
    "ui5-middleware-livereload": "^0.4.1",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-ui5": "^2.2.0",
    "npm-run-all": "^4.1.5",
